{
  "name": "event-manager",
  "version": "1.0.0",
  "description": "Event manager application",
  "main": "Server/bin",
  "scripts": {
    "start": "nodemon ./Server/dist/bin/server.js",
    "build": "babel Server -d Server/dist --ignore Server/dist",
    "postinstall": "npm run build",
    "start:dev": "nodemon --watch Server/ --exec babel-node  -- Server/bin/server.js",
    "test": "nyc --reporter=html --reporter=text mocha --timeout 250000 ./Server/tests/ --compilers js:babel-register",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nzediegwu1/EventManager.git"
  },
  "keywords": [
    "events",
    "facility",
    "centers",
    "locations",
    "fullstack"
  ],
  "author": "Anaeze Nsoffor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nzediegwu1/EventManager/issues"
  },
  "homepage": "https://eventmanager29.herokuapp.com",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "nodemon": "^1.12.1",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.23.1",
    "sequelize-cli": "^3.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-config-defaults": "^9.0.0",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.3.0"
  }
}
